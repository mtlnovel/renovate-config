{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
	  "github>frutbits/renovate-config"
	],
	"hostRules": [
	  {
		"matchHost": "https://npm.pkg.github.com/",
		"hostType": "npm",
		"encrypted": {
		  "token": ""
		}
	  }
	],
	"npmrc": "@mtlnovel:registry=https://npm.pkg.github.com/",
	"packageRules": [
		{
			"matchPackagePatterns": ["@mtlnovel"],
			"groupName": "MTLNovel Packages"
		}
	]
  }